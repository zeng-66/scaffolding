<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pro.mapper.BackUserMapper">
    <sql id="tableName">
        sys_back_user
    </sql>

    <sql id="baseColumn">
        id,org_code,user_name,password,real_name,head_image,last_login_time,last_login_ip,status,roles,is_delete,create_time,modify_time,create_id,modify_id
    </sql>
    <delete id="delById">
        delete from sys_back_user where id = #{id}
    </delete>

</mapper>